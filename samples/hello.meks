class MyClass {
    init() {
        this.x = "Hello from Mek#";
    }

    sayHello() {
        print(this.x);
    }
}

// Create an object
var obj = MyClass();

// Create a closure
var closure = obj.sayHello;
closure();

// Using inheritance
class ExtendedClass < MyClass {
    sayBye() {
        print("Goodbye from Mek#");
    }
}

var child = ExtendedClass();
child.sayBye();
